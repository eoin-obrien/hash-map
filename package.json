{
  "name": "hash-map",
  "version": "0.1.0",
  "license": "MIT",
  "author": "Eoin O'Brien",
  "main": "dist/index.js",
  "module": "dist/hash-map.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "lint": "dts lint",
    "prepare": "dts build",
    "size": "size-limit",
    "start": "dts watch",
    "test": "dts test",
    "commit": "commit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "dts lint",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "engines": {
    "node": ">=12"
  },
  "size-limit": [
    {
      "path": "dist/hash-map.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/hash-map.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@commitlint/prompt-cli": "^16.2.1",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@tsconfig/recommended": "^1.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "dts-cli": "^1.4.0",
    "husky": "^7.0.4",
    "size-limit": "^7.0.8",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
